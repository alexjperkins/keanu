### --- Keanu Alias File --- ###

## ls
alias ls='ls --color=auto'
alias la='ls -a'
alias ll='ls -l'
alias lla='ls -la'


## git
alias gs='git status'
alias gl='git log'
alias gch='git checkout'
alias ga='git add'
alias gpl='git pull'
alias gpo='git push origin'
alias gc='git commit'


## docker
alias dc='docker-compose'


## zathura
alias zath='zathura'


## misc	
alias todo='vim $TODOPATH'
